{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "Book": {
            "additionalProperties": false,
            "properties": {
                "adult": {
                    "type": "boolean"
                },
                "backdrop_path": {
                    "type": "string"
                },
                "genre_ids": {
                    "items": {
                        "type": "number"
                    },
                    "type": "array"
                },
                "id": {
                    "type": "number"
                },
                "original_language": {
                    "type": "string"
                },
                "original_title": {
                    "type": "string"
                },
                "overview": {
                    "type": "string"
                },
                "popularity": {
                    "type": "number"
                },
                "poster_path": {
                    "type": "string"
                },
                "release_date": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "video": {
                    "type": "boolean"
                },
                "vote_average": {
                    "type": "number"
                },
                "vote_count": {
                    "type": "number"
                }
            },
            "required": [
                "adult",
                "genre_ids",
                "original_language",
                "original_title",
                "overview",
                "popularity",
                "release_date",
                "title",
                "vote_average",
                "vote_count"
            ],
            "type": "object"
        }
    }
}

